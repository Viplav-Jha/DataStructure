//String Anagram
// 'hello'--> 'llheo'
// 'viplav'-->'iplavv'

//condition
//length check(for both String )
// String 'hello'
//{h:1,e:1,l:2,o:1}

function isAnagram(string1,string2){
  if(string1.length!=string2.length)
  {
      return false;
  }
  let counter={}
  for(let letter of string1){
  counter[letter]=counter[letter] || 0;

  console.log(counter[letter])
  
  }
}

 isAnagram("hello","llheo")
